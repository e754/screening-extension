{
    "manifest_version": 3,
    "name": "Convidence Screening Support",
    "version": "1.0.0",
    "description": "Helps screen for you",
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "icons" : {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
