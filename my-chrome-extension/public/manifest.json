{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
